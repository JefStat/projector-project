extends layout

block content
  script(type="text/javascript").
    $(document).ready(function(){
      console.log('Starting slick');  
      $('#slick').slick();
    });

  h1= title
  p Welcome to #{title}
  div
    div(data-slick='{"slidesToShow": 1, "slidesToScroll": 1, "autoplay": true, "autoplaySpeed": 3000, "arrows":false}')#slick
      each image, index in images
        img(src=image)


